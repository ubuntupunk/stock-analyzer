{
  "name": "stock-analyzer",
  "version": "1.0.0",
  "description": "Comprehensive stock analysis platform with DCF valuation, factor screening, and portfolio management",
  "scripts": {
    "deploy": "./deploy.sh",
    "deploy-backend": "cd infrastructure && sam build && sam deploy",
    "deploy-frontend": "cd frontend && aws s3 sync . s3://$(aws cloudformation describe-stacks --stack-name stock-analyzer-stack --query 'Stacks[0].Outputs[?OutputKey==`FrontendBucketName`].OutputValue' --output text)/ --delete",
    "logs": "sam logs -n StockAPIFunction --stack-name stock-analyzer-stack --tail",
    "validate": "cd infrastructure && sam validate",
    "clean": "rm -rf infrastructure/.aws-sam",
    "test-api": "curl $(aws cloudformation describe-stacks --stack-name stock-analyzer-stack --query 'Stacks[0].Outputs[?OutputKey==`APIEndpoint`].OutputValue' --output text)/api/stock/metrics?symbol=AAPL",
    "build:css": "cd infrastructure/frontend && sass --no-source-map scss/main.scss:styles.css --style=compressed",
    "watch:css": "cd infrastructure/frontend && sass --no-source-map --watch scss:.",
    "lint": "eslint infrastructure/frontend/**/*.js",
    "lint:fix": "eslint --fix infrastructure/frontend/**/*.js",
    "format": "prettier --write infrastructure/frontend/**/*.js",
    "format:check": "prettier --check infrastructure/frontend/**/*.js",
    "prepare": "husky"
  },
  "keywords": [
    "stock",
    "analysis",
    "DCF",
    "valuation",
    "finance",
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/stock-analyzer"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "sass": "^1.97.3"
  },
  "dependencies": {
    "playwright": "^1.58.1"
  }
}
